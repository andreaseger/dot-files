Host *
  ControlMaster auto
    ControlPath /tmp/m-%l-%r@%h:%p
    ControlPersist 5m
    TCPKeepAlive yes
  ForwardAgent yes
  IdentityFile ~/.ssh/id_ecdsa
  Protocol 2
  Compression yes

Host oho.io *.oho.io
  User sch1zo
Host eger.io *.eger.io
  User sch1zo

Host dev dev.experteer.com
  HostName dev.experteer.com
  User aeger
  IdentityFile ~/.ssh/experteer_rsa

Host *.experteer.com
  IdentityFile ~/.ssh/experteer_rsa

Host git.admin.experteer.com
  User git
  IdentityFile ~/.ssh/experteer_rsa

