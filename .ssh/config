Host *
  ControlMaster auto
    ControlPath /tmp/m-%l-%r@%h:%p
    ControlPersist 5m
    TCPKeepAlive yes
  ForwardAgent no
  IdentityFile ~/.ssh/id_rsa
  Protocol 2
  Compression yes

Host github.com *.github.com bitbucket.org *.bitbucket.org
  IdentityFile ~/.ssh/id_rsa
  ForwardAgent no

Host heroku.com *.heroku.com
  IdentityFile ~/.ssh/id_rsa
  ForwardAgent no

Host oho.io *.oho.io
  User sch1zo
  IdentityFile ~/.ssh/id_ed25519

Host nibbler.cubert.oho.io
  Port 4022

Host hermes.cubert.oho.io
  Port 5022

Host eger.io *.eger.io
  User sch1zo
  IdentityFile ~/.ssh/id_ed25519

Host runtastic.com *.runtastic.com
  User ane
