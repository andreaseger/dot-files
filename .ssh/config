Host *
  ControlMaster auto
    ControlPath /tmp/m-%l-%r@%h:%p
    ControlPersist 5m
    TCPKeepAlive yes
  ForwardAgent yes
  IdentityFile ~/.ssh/id_rsa
  Protocol 2
  Compression yes

Host github.com *.github.com bitbucket.org *.bitbucket.org
  IdentityFile ~/.ssh/id_rsa
  ForwardAgent no

Host heroku.com *.heroku.com
  IdentityFile ~/.ssh/id_rsa
  ForwardAgent no

Host oho.io *.oho.io
  User sch1zo
  IdentityFile ~/.ssh/id_ecdsa
Host eger.io *.eger.io
  User sch1zo
  IdentityFile ~/.ssh/id_ecdsa

