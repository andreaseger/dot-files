!-- Xft settings -- !
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintslight

! -- Fonts -- !
URxvt.font: xft:Consolas:pixelsize=12:style=Regular
URxvt.boldFont: xft:Consolas:pixelsize=12:style=Bold

! -- IR_Black -- !
URxvt*background: #000000
URxvt*foreground: #FFFFFF
! black
URxvt*color0:   #4E4E4E
URxvt*color8:   #7C7C7C
! red
URxvt*color1:   #FF6C60
URxvt*color9:   #FFB6B0
! green
URxvt*color2:   #A8FF60
URxvt*color10:  #CEFFAB
! yellow
URxvt*color3:   #FFFFB6
URxvt*color11:  #FFFFCB
! blue
URxvt*color4:   #96CBFE
URxvt*color12:  #B5DCFE
! magenta
URxvt*color5:   #FF73FD
URxvt*color13:  #FF9CFE
! cyan
URxvt*color6:   #C6C5FE
URxvt*color14:  #DFDFFE
! white
URxvt*color7:   #EEEEEE
URxvt*color15:  #FFFFFF


URxvt.saveLines: 1000
URxvt.scrollBar: false
URxvt.foreground: white
URxvt.background: black
URxvt.secondaryScroll: true
URxvt.perl-ext-common: default,matcher
URxvt.urlLauncher: chromium
URxvt.matcher.button: 1
URxvt*inheritPixmap: true
URxvt*shading: 70

! URxvt.tabbed.font: xft:Consolas:pixelsize=12:style=Regular
! URxvt.tabbed.saveLines: 1000
! URxvt.tabbed.scrollBar: false
! URxvt.tabbed.tabbar-fg: 3
! URxvt.tabbed.tabbar-bg: 0
! URxvt.tabbed.tab-fg: 0
! URxvt.tabbed.tab-bg: 1
